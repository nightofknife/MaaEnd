{
    "ScenePrivateWorldEnterMenuList": {
        "doc": "从大世界进入菜单总列表",
        "recognition": {
            "type": "TemplateMatch",
            "param": {
                "roi": [
                    -200,
                    0,
                    200,
                    200
                ],
                "template": [
                    "SceneManager/WorldMenu.png"
                ],
                "green_mask": true
            }
        },
        "action": {
            "type": "ClickKey",
            "param": {
                "key": 27 // ESC
            }
        },
        "post_wait_freezes": {
            "time": 400,
            "target": [
                420,
                -100,
                450,
                100
            ]
        },
        "next": [
            "ScenePrivateWorldEnterMenuListSuccess"
        ]
    },
    "ScenePrivateWorldEnterMenuListSuccess": {
        "doc": "成功进入菜单总列表",
        "recognition": {
            "type": "OCR",
            "param": {
                "roi": [
                    420,
                    -100,
                    450,
                    100
                ],
                "expected": [
                    "ENDFIELD"
                ]
            }
        }
    },
