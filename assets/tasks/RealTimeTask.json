                {
                    "name": "CheckInterval",
                    "label": "$option.AutoTaskInterval.inputs.CheckInterval.label",
                    "description": "$option.AutoTaskInterval.inputs.CheckInterval.description",
                    "pipeline_type": "int",
                    "verify": "^\\d+$",
                    "default": 400
                }
            ],
            "pipeline_override": {
                "RealTimeTaskMain": {
                    "post_delay": "{CheckInterval}"
                }
            }
